{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/leaderboard",
        "dest": "/server.js"
      },
      {
        "src": "/api/user/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/",
        "dest": "/server.js"
      }
    ]
  }
  